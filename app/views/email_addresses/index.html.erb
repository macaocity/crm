<p id="notice"><%= notice %></p>

<h1><%= t('Listing Email Addresses') %></h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    
    <% @email_addresses.each do |email_address| %>
      <tr>
        <td width="300px"><%= email_address.email %></td>
        
        
        
        <!-- <td><%= link_to 'Show', contact_email_address_path(@contact,email_address) %></td> -->
        <td width="50px"><%= link_to t('Edit'), edit_contact_email_address_path(@contact,email_address) %></td>
        <td width="50px"><%= link_to t('Destroy'), [@contact,email_address], method: :delete, data: { confirm: 'Are you sure?' } %></td>
        
        
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t('New Email address'), new_contact_email_address_path %>&nbsp&nbsp|&nbsp&nbsp 
<%= link_to t('Back To Contact'), contact_path(@contact) %>
